{
  "name": "mc-explorer-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "cytoscape": "^3.33.1",
    "cytoscape-cose-bilkent": "^4.1.0",
    "react": "^19.1.0",
    "react-csv": "^2.2.2",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "client": "cd mc-explorer-client && npm start",
  "server": "cross-env NODE_OPTIONS=--max-old-space-size=8192 node server/server.js",
  "server:dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 nodemon server/server.js",
  "start": "concurrently \"npm run server\" \"npm run client\"",
  "build": "cross-env NODE_OPTIONS=--max-old-space-size=12288 GENERATE_SOURCEMAP=false react-scripts build",
  "test": "cd mc-explorer-client && npm test",
  "eject": "cd mc-explorer-client && npm run eject"
},
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.5",
    "tailwindcss": "^3.4.1"
  }
}
